module rand_digit (
    input in[16],
    output out[7]
) {

    always {
        out = 0;
        case(in) {
            0: out=14b01111110111111;

            1: out=14b01111110000110;
            2: out=14b01111111011011;
            3: out = 14b01111111001111;
            4: out = 14b01111111100110;
            5: out = 14b01111111101101;
            6: out = 14b01111111111101;
            7: out = 14b01111110000111;
            8: out = 14b01111111111111;
            9: out = 14b01111111101111;

            -1: out = 14b10000000000110;
            -2: out = 14b10000001011011;
            -3: out = 14b10000001001111;
            -4: out = 14b10000001100110;
            -5: out = 14b10000001101101;
            -6: out = 14b10000001111101;
            -7: out = 14b10000000000111; 
            -8: out = 14b10000001111111;
            -9: out = 14b10000001101111;

            default: out = 14b00000000000000;
        }
    }
}
